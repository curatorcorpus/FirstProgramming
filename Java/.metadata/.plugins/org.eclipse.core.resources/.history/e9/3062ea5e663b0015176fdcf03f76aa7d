package StopWatchApp2;

import javax.swing.*;

public class StopWatchApp {
	/** main method */
	public static void main(String args[]){
		/** instantiate jframe */
		JFrame frame = new JFrame("Stop Watch Application");
		
		// JFrame settings
		
		frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		frame.pack();
		frame.setVisible(true);
	}
}

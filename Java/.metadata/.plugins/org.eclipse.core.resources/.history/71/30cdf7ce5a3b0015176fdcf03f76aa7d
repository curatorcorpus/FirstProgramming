public class Nanotime{
  public static void main(String args[]){
    int total = 0, second = 0, min = 0, hr = 0,count = 0;
    long start, stop, estimate;
    
    start = System.nanoTime()/1000000000;
    
    while(total < 1000){
    	stop = System.nanoTime()/1000000000;
    	estimate = stop - start;
//    	if((estimate - estimate/1000*1000)/10 >= 99){
//    		System.out.println(estimate - (estimate/1000*1000)/10);
//    	}
    	
    	
    		System.out.println(estimate);
    }
  }
}
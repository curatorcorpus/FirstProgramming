package Coin;

public class MonetaryCoinApp {
	/** main method */
	public static void main(String args[]){
		MonetaryCoin twoDolCoin = new MonetaryCoin(2);
		MonetaryCoin oneDolCoin = new MonetaryCoin(1);
		MonetaryCoin fiftyDolCoin = new MonetaryCoin(50);
		
		System.out.println(twoDolCoin.getCoinVal()+oneDolCoin.getCoinVal()+
				fiftyDolCoin.getCoinVal());
	}
}

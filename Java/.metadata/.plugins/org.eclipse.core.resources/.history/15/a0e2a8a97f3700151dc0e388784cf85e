 * using key events and key bindings
 */
import javax.swing.*;

public class MoveApp {
	/** main method */
	public static void main(String args[]){
		// instantiation of JFrame
		JFrame frame = new JFrame("I Like to Move it Move it!");
		
		frame.getContentPane().add(new MovePanel()); // add jpanel
		frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE); // close normally
		frame.pack(); // packs all contents in prefered size
		frame.setVisible(true); // allows you to see gui
	}
}

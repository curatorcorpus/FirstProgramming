
public class TEst {

	public static void main(String args[]){
		int rows = 5, cols = 4;
		
		int[][] table = new int[rows][cols];
		for(int row = 0; row < rows; row++){
			for(int col = 0; col < cols; col++){
				table[row][col] = row * col;
			}
		}
		
		for(int[] row : table){
			System.out.println(row)
			for(int col : row){
				System.out.println("\t"+col);
			}
		}
	}
}

package Sample2;

public class Practice {
    public static void main(String[] args){
	static int = 0;
	System.out.println(maximum(2,4,1));
    }
    
    public static int maximum(int i, int j, int k){
	int max = 0;
	if(i < j && k < j){
	    max = j;
	}
	
	else if(j < i && k < i){
	    max = i;
	}
	
	else if(i < k && j < k){
	    max = k;
	}
	
	return max;
    }
}

package catchme;
import javax.swing.*;

public class MonsterApp{
	/** main method */
	public static void main(String arg[]){
		// instantiation of jframe
		JFrame frame = new JFrame("Catch The Monster Game");
		
		// jframe settings
		frame.setContentPane.add(new GamePanel());
		frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		frame.pack();
		frame.setVisible(true);
	}
}

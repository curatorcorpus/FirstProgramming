public class Nanotime{
  public static void main(String args[]){
    int estimate, second = 0;
    double start, stop;
    
    start = System.nanoTime()/1000000000.0;
    
    while(true){
    	stop = System.nanoTime()/1000000000.0;
    	estimate = stop - start;
    	second++;
    	System.out.println(second);
    }
  }
}